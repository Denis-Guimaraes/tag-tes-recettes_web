{
  "name": "tag-tes-recettes-web",
  "version": "0.0.0",
  "private": true,
  "devDependencies": {
    "@babel/core": "*",
    "@babel/plugin-proposal-class-properties": "*",
    "@babel/plugin-proposal-object-rest-spread": "*",
    "@babel/preset-env": "*",
    "@babel/preset-react": "*",
    "autoprefixer": "*",
    "babel-eslint": "*",
    "babel-jest": "^24.5.0",
    "babel-loader": "*",
    "css-loader": "*",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.11.2",
    "enzyme-to-json": "^3.3.5",
    "eslint": "*",
    "eslint-config-airbnb": "*",
    "eslint-import-resolver-webpack": "*",
    "eslint-plugin-import": "*",
    "eslint-plugin-jsx-a11y": "*",
    "eslint-plugin-react": "*",
    "file-loader": "*",
    "html-webpack-plugin": "*",
    "ignore-styles": "*",
    "jest": "^24.5.0",
    "jest-webpack-resolver": "^0.3.0",
    "mini-css-extract-plugin": "*",
    "node-sass": "*",
    "optimize-css-assets-webpack-plugin": "*",
    "postcss": "*",
    "postcss-loader": "*",
    "sass-loader": "*",
    "style-loader": "*",
    "stylelint": "*",
    "stylelint-config-recommended": "*",
    "uglifyjs-webpack-plugin": "*",
    "webpack": "*",
    "webpack-cli": "*",
    "webpack-dev-server": "^3.1.11"
  },
  "dependencies": {
    "@babel/polyfill": "*",
    "axios": "*",
    "classnames": "*",
    "prop-types": "*",
    "react": "*",
    "react-dom": "*",
    "react-icons": "^3.3.0",
    "react-redux": "*",
    "react-router-dom": "^4.3.1",
    "redux": "*",
    "uuid": "^3.3.2"
  },
  "scripts": {
    "start": "webpack-dev-server",
    "clean": "rm -rf dist",
    "clean:all": "rm -rf dist node_modules",
    "build:dev": "NODE_ENV=development webpack",
    "build:dev:win": "set NODE_ENV=development && webpack",
    "build:prod": "NODE_ENV=production webpack --mode production",
    "build:prod:win": "set NODE_ENV=production && webpack --mode production",
    "save-deps": "rm -rf node_modules && npm update --save && npm update --save-dev",
    "test": "jest --collectCoverage=true"
  },
  "jestWebpackResolver": {
    "webpackConfig": "webpack.config.js"
  },
  "jest": {
    "verbose": true,
    "resolver": "jest-webpack-resolver",
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/components/__mocks__/fileMock.js",
      "\\.(css|less|scss)$": "<rootDir>/src/components/__mocks__/styleMock.js"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/enzyme.config.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  }
}
